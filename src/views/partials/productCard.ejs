<div class="productContainer">
    <div class="cardContainer">
            <a href="/products/<%= producto.id %>">
                <img class="imgdestacado" src="<%= producto.centralImage || 'https://i.pinimg.com/originals/a1/3d/28/a13d2824e7dc14007cf4f35a9017989f.jpg' %>" alt="<%= producto.name %>">
                <div class="nombre"><h5><%= producto.name %></h5></div>
                <div class="precio"><%= producto.price %></div>
            </a>
    </div>
</div>